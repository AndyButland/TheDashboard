  <Actions>
    <Action runat="install" alias="addDashboardSection" dashboardAlias="TheDashboard">
      <section>
        <areas>
          <area>content</area>
        </areas>
        <tab caption="Welcome">
          <control showOnce="true" addPanel="true" panelCaption="">
            /app_plugins/TheDashboard/TheDashboard.html
          </control>
        </tab>
      </section>
    </Action>
    <Action runat="install" alias="addDashboardSection" dashboardAlias="TheDevDashboard">
